pipeline {
    agent any

    stages {
        stage('Dependencies') {
            steps {
                sh 'cd JSPaint && npm i'
            }
        }
        stage('Build') {
            steps {
                sh 'cd JSPaint && npm ci'
                
            }
        }
        stage('Test') {
            steps {
                sh 'cd JSPaint'
            }  
        }

    }
}
